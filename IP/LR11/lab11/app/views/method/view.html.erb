<h3>Страница для вывода результатов подсчета</h3>
<p id="result">
  <!-- <%= debug(@result) %> -->
  <% if @result.is_a?(String) %>
  <%= @result%>
  <%else%>
  <table>
    <% @result.each_with_index do |elem, index| %>
    <tr>
      <td><%= index+1%></td>
      <td><%= elem%></td>
      <td><%= index == @result.size-1 ? 'YES' : 'NO'%></td>
    </tr>
    <% end %>
  </table>
  <% end %>
</p>
